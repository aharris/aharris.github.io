//Custom
.svg-icon
  display block

.social-line
  display inline-block
  vertical-align top
  opacity .6
  width 46px
  height 74px
  overflow hidden
  margin 0 10px
  transition .2s
  +above(768px)
    width 80px
    height 80px
    margin 20px

  &:hover
    opacity 1

  .svg_long_shadow
